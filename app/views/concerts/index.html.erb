<table>
  <tr>
    <th> Artist </th>
    <th> Schedule </th>
    <th> City </th>
    <th> State </th>
  </tr>
  <% @concerts.each do |concert| %>
    <tr>
      <td><%= concert.artist %></td>
      <td><%= concert.schedule %></td>
      <td><%= concert.city %></td>
      <td><%= concert.state %></td>
    </tr>
  <% end %>
</table>
        
      